@model DichVu
@Html.HiddenFor(x => x.MaDichVu)

<div class="box-body">
    <div class="form-group">
        <label class="col-sm-3 control-label">Tên dịch vụ</label>
        <div class="col-sm-9">
            @Html.TextBoxFor(x => x.TenDichVu, new Dictionary<string, object>
            {
                {"class", "form-control"}
            })
            @Html.ValidationMessageFor(x => x.TenDichVu)
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-3 control-label">Giá (VNĐ)</label>
        <div class="col-sm-9">
            @Html.TextBoxFor(x => x.Gia, new Dictionary<string, object>
            {
                {"class", "form-control"}
            })
            @Html.ValidationMessageFor(x => x.Gia)
        </div>
    </div>
</div>
